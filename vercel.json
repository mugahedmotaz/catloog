{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://catloog.com/$1",
      "permanent": true,
      "has": [
        { "type": "host", "value": "www.catloog.com" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
